#!/usr/bin/env bash
# shellcheck disable=SC1091
source ember_bash_setup &> /dev/null
trap 'die "A fatal error was reported on ${BASH_SOURCE[0]} line ${LINENO}."' ERR

set -x

sourceDir="$1"

destDir="$2"

maxPostsPerPage="5"

[[ -e "$2" ]] && rm -r "$2"

rsync -a --checksum "$1/" "$2"

cd "$2" || die



echo "built" > ".futuramerlin-web-toolkit-blog.conf"
